package validation;

import validation.Validator;

public class ValidationProcess {
	private Validator objValidator;
	
	public ValidationProcess(String[] strInArrayPrimaryModelNames) {
		System.out.println("Generating a Validator object...");
		this.objValidator = new Validator(strInArrayPrimaryModelNames);
	}
	
	public void start() {
		this.objValidator.run();
	}
}
