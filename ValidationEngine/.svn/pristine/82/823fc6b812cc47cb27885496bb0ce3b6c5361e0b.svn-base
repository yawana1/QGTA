package data;

import java.nio.file.Path;
import java.util.ArrayList;
import java.util.List;

public class Trial {
	
	private String strName;
	private Path objPath;
	private List<Model> lstModels;
	
	public Trial(String strInName,
			     Path objInPath)
	{
		this.strName = strInName;
		this.objPath = objInPath;
		this.lstModels = new ArrayList<Model>();
	}

	public void addModel(Model objInModel)
	{
		this.lstModels.add(objInModel);
	}
	
	public String getName() {
		return this.strName;
	}
	
	public Path getPath() {
		return this.objPath;
	}
	
	public List<Model> getModels() {
		return this.lstModels;
	}
}
